{
  "expo": {
    "name": "Reminder",
    "slug": "reminder",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./fonte/content/logoapp.png",
    "splash": {
      "image": "./fonte/content/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gbp.reminder",
    "versionCode": 1,

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
